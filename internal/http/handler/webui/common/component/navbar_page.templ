package component

templ NavbarPage(navbar NavbarVModel, funcs ...PageOptionFunc) {
	@Page(funcs...) {
		{{ isDesktopApp := IsDesktopApp(ctx) }}
		<div class={ "container", templ.KV("is-fluid", isDesktopApp) }>
			@Navbar(navbar)
			<section class="section pt-3 px-0">
				{ children... }
			</section>
		</div>
	}
}
